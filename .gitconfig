# This is Git's per-user configuration file.
[user]
  name = sutekami
[alias]
  pretty-log = log --graph --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)'
[core]
  editor = nvim
[include]
  path = ~/.gitconfig.local
